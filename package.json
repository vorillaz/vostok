{
  "name": "vostok",
  "version": "1.3.0",
  "repository": "vorillaz/vostok",
  "author": "@vorillaz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vorillaz/vostok/issues"
  },
  "bin": {
    "vostok": "./bin/vostok"
  },
  "scripts": {
    "test": "ava",
    "test:watch": "ava --watch --verbose"
  },
  "dependencies": {
    "boxen": "4.2.0",
    "chalk": "3.0.0",
    "clipboardy": "2.2.0",
    "cross-spawn": "7.0.1",
    "dotenv": "8.2.0",
    "fastify": "2.12.0",
    "fastify-http-proxy": "3.0.0",
    "fastify-static": "2.6.0",
    "fs-extra": "8.1.0",
    "minimist": "1.2.5",
    "semver": "7.1.3"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.2.0",
    "ava": "3.8.2",
    "husky": "4.2.3",
    "lint-staged": "10.0.7",
    "prettier": "1.19.1"
  },
  "engines": {
    "node": "10.x"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,md}": [
      "prettier --write"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.js"
    ],
    "concurrency": 5,
    "verbose": true,
    "ignoredByWatcher": [
      "test/fixtures"
    ]
  }
}
